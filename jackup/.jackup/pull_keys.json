[
    {
        "name": "ssh",
        "source": "~/.ssh/",
        "destination": "~/private/.keys/.ssh/",
        "order": 0
    },
    {
        "name": "gpg",
        "source": "~/.gnupg/",
        "destination": "~/private/.keys/.gpg/",
        "order": 1
    },
    {
        "name": "pass",
        "source": "~/.password-store/",
        "destination": "~/private/.keys/.pass/",
        "order": 2
    }
]
