#!/bin/bash

INTERFACE="wlan0"
STRENGTH=$(cat /proc/net/wireless | grep $INTERFACE | awk '{print $3}' | tr -d '.')

if [ -z $STRENGTH ]; then
    echo ""
elif [ $STRENGTH -lt 25 ]; then
    echo ""
elif [ $STRENGTH -lt 50 ]; then
    echo ""
elif [ $STRENGTH -lt 75 ]; then
    echo ""
else
    echo ""
fi
